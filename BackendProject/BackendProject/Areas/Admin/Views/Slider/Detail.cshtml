@model Slider
@{
    ViewData["Title"] = "Detail";
}
<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-6">
            <div style="width:200px; height:auto">
                <img src="~/assets/images/@Model.UserImage" />
            </div>
            <h4>Title: @Model.UserName</h4>
            <h4>Description: @Model.Description</h4>
            <h4>Offer: @Model.Designation</h4>

            <a class="btn btn-info" asp-action="Index">Go Back</a>
        </div>
    </div>
</div>

